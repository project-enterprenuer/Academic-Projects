for n=2:6
    d=0:0.01:1
    num=d.*(d-1).*(d-1);
    den=2.*((2.*n)+1).*((n.*d)n+1);
  tlmb=num./den;
  if n<=2
      hold;
  end
  plot(d,tlmb);
end
xlabel('DUTY CYCLE');
ylabel('TLMB');
